{
  "name": "raw-pouch-test",
  "description": "Bare bones JS to test isomorphic use of pouchdb",
  "version": "0.1.0",
  "dependencies": {
    "brotli": "^1.3.3",
    "dotenv": "^16.0.1",
    "fs-extra": "^11.1.0"
  },
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch ./src -e css,mjs --experimental-network-imports -r dotenv/config server.mjs",
    "prod": "NODE_ENV=production node --experimental-network-imports ./server.mjs",
    "test": "tap"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.14",
    "postcss-minify": "^1.1.0",
    "postcss-nesting": "^10.2.0",
    "puppeteer": "^19.10.1",
    "tap": "^16.3.4",
    "uglify-js": "^3.17.4"
  }
}
